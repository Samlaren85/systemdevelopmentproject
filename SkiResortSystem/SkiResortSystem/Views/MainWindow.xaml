        </TabItem>
        <TabItem Header="kund" BorderBrush="#FF5E8CE2">
            <Button DockPanel.Dock="Bottom" Height="25" Width="100" Margin="650,300,10,10" Content="Logga ut"/>

        </TabItem>

        <TabItem Header="boende/konferens" Height="106" Margin="-2,-2,-2,0" VerticalAlignment="Top" BorderBrush="#FF5E8CE2"></TabItem>

        <Button DockPanel.Dock="Bottom" Height="25" Width="100" Margin="650,300,10,10" Content="Logga ut"/>









    </TabControl>


